{
  "name": "Choir Music Search",
  "description": "Full-stack choir music search application with React frontend and Node.js backend",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "VITE_BUILD_MODE=production npm run build",
    "preview": "vite preview",
    "test": "vitest",
    "status": "startup-summary.bat",
    "backend": "cd backend && npm run dev",
    "setup": "npm install && cd backend && npm install",
    "deploy:onecom": "npm run build:prod && node scripts/deploy-onecom.js",
    "deploy:preview": "npm run build:prod && node scripts/deploy-onecom.js --preview",
    "deploy:dry-run": "npm run build:prod && node scripts/deploy-onecom.js --dry-run"
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.89.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "ftp-deploy": "^2.4.7",
    "postcss": "^8.4.27",
    "supabase": "^2.40.7",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^6.3.6"
  }
}
