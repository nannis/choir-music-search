{
  "name": "Choir Music Search",
  "description": "Full-stack choir music search application with React frontend and Node.js backend",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "npm run build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage-check": "node scripts/check-coverage.js",
    "test:ui": "vitest --ui",
    "test:frontend": "node scripts/test-frontend-automated.js",
    "test:accessibility": "node scripts/test-accessibility.js",
    "test:accessibility-simple": "node scripts/test-accessibility-simple.js",
    "test:frontend-changes": "node scripts/test-frontend-automated.js --changes-only",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "test:generate": "node scripts/generate-test.js",
    "status": "startup-summary.bat",
    "backend": "cd backend && npm run dev",
    "setup": "npm install && cd backend && npm install",
    "update-version": "node scripts/update-version.js",
    "pre-deploy": "npm run lint && npm run test:coverage && npm run test:frontend && npm run update-version && npm run build:prod",
    "deploy:onecom": "npm run pre-deploy && node scripts/deploy-onecom.js",
    "deploy:preview": "npm run pre-deploy && node scripts/deploy-onecom.js --preview",
    "deploy:dry-run": "npm run pre-deploy && node scripts/deploy-onecom.js --dry-run",
    "deploy:git": "bash scripts/deploy-git.sh",
    "deploy:git-win": "scripts\\deploy-git.bat",
    "deploy:onecom-git": "scripts\\deploy-onecom-git.bat",
    "setup:ssh": "scripts\\setup-ssh-keys.bat",
    "prepare": "husky",
    "test:unit": "vitest run tests/unit/",
    "test:integration": "vitest run tests/integration/",
    "test:backend": "vitest run tests/backend/",
    "test:frontend-only": "vitest run tests/frontend/",
    "test:accessibility-only": "vitest run tests/frontend/accessibility.test.tsx",
    "test:edge-functions": "vitest run tests/edge-function*.test.ts",
    "test:data-ingestion": "vitest run tests/data-ingestion*.test.ts",
    "test:api-contract": "vitest run tests/api-contract.test.ts",
    "test:deployment": "vitest run tests/deploy-*.test.ts",
    "test:quick": "vitest run --reporter=basic",
    "test:watch": "vitest --watch",
    "test:watch:unit": "vitest --watch tests/unit/",
    "test:watch:frontend": "vitest --watch tests/frontend/",
    "test:watch:backend": "vitest --watch tests/backend/",
    "test:rls": "node scripts/test-rls-policies.js",
    "test:rls-policies": "vitest run tests/rls-policies.test.ts"
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.58.0",
    "@tanstack/react-query": "^5.89.0",
    "@types/node": "^24.5.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "lovable-tagger": "^1.1.9",
    "lucide-react": "^0.263.1",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^5.0.3",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.15.5",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "express": "^5.1.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.1.6",
    "mysql2": "^3.15.0",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "playwright": "^1.55.0",
    "postcss": "^8.4.27",
    "ssh2-sftp-client": "^12.0.1",
    "supabase": "^2.40.7",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^6.3.6",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "npm run test:frontend-changes"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  }
}
